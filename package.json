{
  "name": "monorepo",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:gateway\" \"pnpm run bw workspace dev\"",
    "dev:gateway": "npx nx run-many --target=dev --projects=viewer",
    "bw": "./node_modules/bos-workspace/bin/bw.js",
    "build": "concurrently \"pnpm run -r build\" \"pnpm run bw build\""
  },
  "devDependencies": {
    "bos-workspace": "workspace:*",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "nx": "^18.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "workspaces": [
    "gateway",
    "VM",
    "packages/*"
  ]
}
